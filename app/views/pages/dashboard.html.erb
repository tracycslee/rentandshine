<h1>Welcome <%= @user.email%>!</h1>
<h2>Your Bookings</h2>
<!-- booking card -->
<div class="container">
  <div class="row">
    <% @bookings.each do |booking| %>
      <div class="col-xs-12 col-sm-6 col-md-4">
        <div class="card" >
          <%= cl_image_tag booking.image, class: "card-img" %>
          <div class="card-description">
            <p id="brand"><%=booking.brand%></p>
            <p><i><%=booking.detail%></i></p>
            <p><%=booking.size%></p>
            <p>$<%=booking.price%>/day</p>
          </div>
        </div>
        <%= link_to "", booking_path(booking), class: 'card-link' %>
      </div>
    <% end %>
  </div>
</div>
<% @bookings.each do |booking| %>
  <% booking. %>
<% end %>
<h2>Your Listings</h2>
<!-- listing card -->
<div class="container">
  <div class="row">
    <% @listings.each do |listing| %>
      <div class="col-xs-12 col-sm-6 col-md-4">
        <div class="card" >
          <%= cl_image_tag listing.image, class: "card-img" %>
          <div class="card-description">
            <p id="brand"><%=listing.brand%></p>
            <p><i><%=listing.detail%></i></p>
            <p><%=listing.size%></p>
            <p>$<%=listing.price%>/day</p>
          </div>
        </div>
        <%= link_to "", listing_path(listing), class: 'card-link' %>
      </div>
    <% end %>
  </div>
</div>
